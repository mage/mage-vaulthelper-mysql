{
    "compilerOptions": {
      "declaration": true,
      "alwaysStrict": true,
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "removeComments": false,
      "inlineSources": true,
      "inlineSourceMap": true
    }
  }